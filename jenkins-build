
properties([pipelineTriggers([githubPush()])])
 
pipeline {
    /* specify nodes for executing */
    agent any
 
    stages {
        
         stage('Web Page Test') {
            steps {
                webpagetest test https://www.google.com/search?q=mac+m1+chip --poll --wait -k %WPT_API_KEY%
            }
        }
    }
 
    /* Cleanup workspace */
    post {
       always {
           deleteDir()
       }
   }
}

